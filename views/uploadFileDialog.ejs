<dialog id="uploadFileDialog">


    <div class="dialogContents">
        <div class="dialogHeader">
            <div class="dialogHeading">Upload File</div>
            <button class="cancelButton dialogCloseButton">X</button>
        </div>
        
        <div class="dialogSubHeading">Max File Size : 10Mb</div>
        
        <form action="<%=uploadLink%>" method="POST" enctype="multipart/form-data">
            <input type="file" name="uploaded-file" id="uploaded-file" required/>
            <button class="uploadFileButton submitButton" type="submit">UPLOAD</button>
        </form>
        

    </div>
    
  </dialog>

